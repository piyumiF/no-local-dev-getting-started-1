<div class="container">

<table class="table table-striped table-bordered">
<tr>
<th>First Name</th>
<th>Last Name</th>
<th>Email</th>
<th>Mobile</th>
</tr>

<% @contacts.each do |contact| %>
<tr>
<td><%= contact.firstname %></td>
<td><%= contact.lastname %></td>
<td><%= contact.email %></td>
<td><%= contact.mobilephone %></td>
</tr>
<% end %>
</table>

  
</div>
